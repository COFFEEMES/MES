<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
  <mapper namespace="com.yedam.spring.material.mapper.MaterialMapper">
  
  <!-- 발주조회 -->
	<select id="selectMaterialOrderList" resultType="MaterialOrderVO">
		SELECT ordr_cd,
			   ordr_req_dt,
			   vend_cd
	    FROM rsc_ordr
	    ORDER BY ordr_cd
	</select>
  
